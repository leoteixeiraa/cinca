his.numero,\n        pontoReferencia: this.pontoReferencia,\n        cep: this.cep,\n        uf: this.uf,\n        email: this.email,\n        telefone: this.telefone,\n        site: this.site,\n        observacoes: this.observacoes,\n        idPJuridica: this.idPJuridica\n      };\n      this.provider.Api(dados2, 'apiPessoaJuridica.php')\n        .subscribe(data => {\n\n          if (data['success']) {\n            alert('Editado com sucesso!!');\n\n            //  location='linhas';\n            this.router.navigate(['/pessoa-juridica']);\n          } else {\n            alert('Erro ao Editar!!');\n          }\n\n        });\n    });\n\n  }\n\n  excluir(idu: string) {\n    return new Promise(resolve => {\n      const dados2 = {\n        requisicao: 'excluir',\n        idPJuridica: idu\n      };\n      this.provider.Api(dados2, 'apiPessoaJuridica.php')\n        .subscribe(data => {\n\n          if (data['success']) {\n            alert('Excluido com sucesso!');\n\n            window.location.href = \"pessoa-juridica\";\n          } else {\n            alert('Erro ao Excluir!!');\n          }\n\n        });\n    });\n  }\n\n}\n","<!-- Toolbar -->\n<nav class=\"navbar navbar-expand-sm navbar-dark bg-dark\">\n  <img src=\"https://www.cincatarina.sc.gov.br/images/cincatarina/logo-light.png\"\n    class=\"img-fluid ${3|rounded-top,rounded-right,rounded-bottom,rounded-left,rounded-circle,|}\" alt=\"\">\n  <a></a>\n  <button class=\"navbar-toggler d-lg-none\" type=\"button\" data-toggle=\"collapse\" data-target=\"#collapsibleNavId\"\n    aria-controls=\"collapsibleNavId\" aria-expanded=\"false\" aria-label=\"Toggle navigation\"></button>\n  <div class=\"collapse navbar-collapse\" id=\"collapsibleNavId\">\n    <ul class=\"navbar-nav mr-auto mt-2 mt-lg-0\">\n      <li class=\"nav-item active\">\n        <a class=\"nav-link\" href=\"#\">Inicio <span class=\"sr-only\">(current)</span></a>\n      </li>\n      <li class=\"nav-item dropdown\">\n        <a class=\"nav-link dropdo